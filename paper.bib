	year = "1997",
	note = "<ce:title>Supplement to Computers and Chemical Engineering</ce:title> <xocs:full-name>6th International Symposium on Process Systems Engineering and 30th European Symposium on Computer Aided Process Engineering</xocs:full-name>",
